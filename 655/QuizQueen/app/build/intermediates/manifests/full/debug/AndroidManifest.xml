<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sam.quizqueen"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.sam.quizqueen.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sam.quizqueen.DbHelper"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.sam.quizqueen.Question"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.sam.quizqueen.ResultActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.sam.quizqueen.UserManagement"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.sam.quizqueen.AddQuestion"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.sam.quizqueen.QuizManagement"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.sam.quizqueen.QuizTake"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.sam.quizqueen.Stats"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.sam.quizqueen.QuizResult"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.sam.quizqueen.AddUser"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustUnspecified|stateHidden" />
    </application>

</manifest>